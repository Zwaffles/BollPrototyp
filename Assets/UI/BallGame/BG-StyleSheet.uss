    padding-left: 16px;
    padding-right: 32px;
    padding-top: 8px;
    padding-bottom: 8px;
    -unity-text-align: middle-right;
    align-items: center;
    justify-content: center;
    visibility: visible;
    overflow: visible;
    -unity-background-scale-mode: stretch-to-fill;
    border-left-width: 0;
