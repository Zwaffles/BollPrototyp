    width: 48px;
    height: 32px;
}

.LS-PBTime {
    font-size: 24px;
    color: rgb(221, 221, 221);
    align-items: center;
    justify-content: center;
    flex-direction: row;
    -unity-text-align: middle-center;
    -unity-background-scale-mode: scale-and-crop;
}

.LS-CourseName {
    font-size: 32px;
    color: rgb(222, 222, 222);
}

.LS-TrackNumber {
